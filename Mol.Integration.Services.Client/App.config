<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <extensions>
      <behaviorExtensions>
        <add name="userNameClientCredentials" type="Mol.Integration.Lib.Binding.UserNameClientCredentialsElement, Mol.Integration.Lib, Version=1.0.0.0, Culture=neutral, PublicKeyToken=6c5105b1c4525fe2" />
      </behaviorExtensions>
    </extensions>
    <behaviors>
      <endpointBehaviors>
        <behavior name="wsHttpBehavior">
          <userNameClientCredentials userName="tamkeen" password="123">
            <serviceCertificate>
              <authentication certificateValidationMode="None" />
            </serviceCertificate>
          </userNameClientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <bindings>
      <basicHttpBinding>
        <binding name="basicHttpEndpoint">
          <security mode="TransportWithMessageCredential" />
        </binding>
      </basicHttpBinding>
      <wsHttpBinding>
        <binding name="wsHttpEndPoint">
          <security>
            <message clientCredentialType="UserName" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://172.26.20.52:9510/NICService.svc" behaviorConfiguration="wsHttpBehavior"
        binding="wsHttpBinding" bindingConfiguration="wsHttpEndPoint"
        contract="NICServiceReference.INICService" name="wsHttpEndPoint">
        <identity>
          <dns value="biztalk-srv3" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>
